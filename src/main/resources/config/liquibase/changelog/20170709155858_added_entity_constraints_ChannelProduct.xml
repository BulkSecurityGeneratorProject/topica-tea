<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity ChannelProduct.
    -->
    <changeSet id="20170709155858-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="channel_product"
                                 constraintName="fk_channel_product_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="ads_type_id"
                                 baseTableName="channel_product"
                                 constraintName="fk_channel_product_ads_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ads_type"/>

    </changeSet>
</databaseChangeLog>
